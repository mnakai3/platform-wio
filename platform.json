{
  "name": "seeedwio",
  "title": "Seeed Wio",
  "description": "Seeed Wio is compatible Arduino board include wireless module. The board implement to many IoT application usecase with wireless communication.",
  "url": "https://seeedjp.github.io/Wiki/",
  "homepage": "https://www.seeed.co.jp/",
  "license": "Apache-2.0",
  "engines": {
    "platformio": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mnakai3/platform-wio"
  },
  "version": "0.1.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json",
    "https://raw.githubusercontent.com/mnakai3/pio-packages/master/manifest.json"
  ],
  "frameworks": {
    "arduino": {
      "package": "framework-arduino-wio",
      "script": "builder/frameworks/arduino.py"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "version": ">=1.60301.0,<1.80000.0"
    },
    "framework-arduino-wio": {
      "type": "framework",
      "optional": true,
      "version": "~1.3.0"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "version": "~2.1000.0"
    },
    "tool-jlink": {
      "type": "uploader",
      "optional": true,
      "version": "^1.63208.0"
    }
  }
}
